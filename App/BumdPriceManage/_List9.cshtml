
@model T_SerYearlyPriceView
 
<div class="netcaretable">
                        <div class="columns  columns-right btn-group pull-left bottombtn" style=" " id="tbar9">
                            <button class="btn btn-default" type="button" title="增加" id="add9" style="display:none" onclick="if($('#SerYearlyPriceId').val()==0){ msg.info('请先保存主数据！');return false;};msg.loadBtn('service/BumdPriceManage/Edit?type=9','新增');">
                                <i class="fa fa-plus"></i>&nbsp;增加
                            </button>
                            <button class="btn btn-default" type="button" title="导入" id="import9" style="display:none" onclick="if ($('#SerYearlyPriceId').val() == 0) { msg.info('请先保存主数据！'); return false; }; msg.loadBtn('admin/SysAttachment/Index?type=9', '新增', 700, 450);">
                                <i class="fa fa-file-excel-o"></i>&nbsp;导入
                            </button>
                            <button class="btn btn-default" type="button" title="下载" id="export9" onclick="location.href='@(Url.Content("~/Images/BUMD/年度特殊经销商价格表.xlsx?3"))' ">
                                <i class="fa fa-download"></i>&nbsp;下载
                            </button>
