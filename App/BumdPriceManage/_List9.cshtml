        }, {
            field: 'EffectiveEnd',
            title: '有效期止',
            align: 'center'
        } , {
            field: 'Remark',
            title: '备注',
            align: 'center'
        }
    ];

	     
	var dbQueryParamsPro9 = function (params) {
		var temp = {};
		temp["PageSize"] = params.limit;
		temp["PageIndex"] = params.offset;
		temp["SerYearlyPriceId"] = $("#SerYearlyPriceId").val(); 
		temp["TableType"] = 9; 
		return temp;
	};
     
    function loadProduct9(fun) {
         var actionUrl = path + 'service/BumdPriceManage/DetailList';
        InitTable($("#productinfo9"), actionUrl, dbQueryParamsPro9, productColumns9, "", "", "tbar9",function(e){
            $("#ba9").html(e.total);
            if(fun){
                fun();
            }
		});
    }
	</script>